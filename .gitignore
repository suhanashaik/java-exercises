public class Reverse
{
public int[] reverseN(int a[],int n)
{
System.out.println("the reversed array elements are:");
int j=0,b[];
b=new int[n];
for(int i=n-1;i>=0;i--)
{
b[j]=a[i];
j++;
}
return b;
}
}
